<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmileyLoadingView">
        <!-- animation duration -->
        <attr name="duration" format="integer" />
        <!-- stroke width -->
        <attr name="strokeWidth" format="dimension" />
        <!-- stroke color -->
        <attr name="strokeColor" format="color" />
        <!-- animation repeat count -->
        <attr name="animRepeatCount" format="integer" />
    </declare-styleable>
    <!-- UITable -->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="XRefreshView">
        <attr name="isHeightMatchParent" format="boolean" />
        <attr name="isWidthMatchParent" format="boolean" />
        <attr name="autoRefresh" format="boolean" />
        <attr name="autoLoadMore" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RingView">
        <attr name="ringtextcolor" format="color" />
        <attr name="pecentColor" format="color" />
    </declare-styleable>
    <declare-styleable name="PlayVideoIconView">
        <attr name="themecolor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="test">
        <attr name="collapsedPadding" format="dimension" />
        <attr name="expandedPadding" format="dimension" />

        <attr name="collapsedImageSize" format="dimension" />

        <attr name="collapsed_text_size" format="dimension"></attr>
        <attr name="expanded_text_size" format="dimension"></attr>

        <attr name="marginbottomoffset" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="Cookie">
        <!--TextColor-->
        <attr name="cookieTitleColor" format="color" />
        <attr name="cookieMessageColor" format="color" />
        <attr name="cookieActionColor" format="color" />
        <attr name="cookieBackgroundColor" format="color" />
        <!--TextSize-->
        <attr name="titleTextSize" format="dimension" />
        <attr name="messageTextSize" format="dimension" />
        <attr name="actionTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MsgView">
        <!-- 圆角矩形背景色 -->
        <attr name="mv_backgroundColor" format="color" />
        <!-- 圆角弧度,单位dp-->
        <attr name="mv_cornerRadius" format="dimension" />
        <!-- 圆角弧度,单位dp-->
        <attr name="mv_strokeWidth" format="dimension" />
        <!-- 圆角边框颜色-->
        <attr name="mv_strokeColor" format="color" />
        <!-- 圆角弧度是高度一半-->
        <attr name="mv_isRadiusHalfHeight" format="boolean" />
        <!-- 圆角矩形宽高相等,取较宽高中大值-->
        <attr name="mv_isWidthHeightEqual" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="animAlphaStart" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="TimeView">
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="isDrawText" format="boolean" />
        <attr name="centerPointColor" format="color" />
        <attr name="centerPointSize" format="dimension" />
        <attr name="centerPointRadiu" format="dimension" />
        <attr name="centerPointType" format="string" />
        <attr name="selectedTextColor" format="color" />
        <!--圆的半径-相对布局宽度的比例用/符号连接（比如:1/3）-->
        <attr name="circleRadiusScale" format="string" />
    </declare-styleable>
</resources>